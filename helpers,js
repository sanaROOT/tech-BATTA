function successfullMessage(msg) {
    return "✅ *x-Troid*:  ```" + msg + "```"
}
function errorMessage(msg) {
    return "🛑 *x-Troid*:  ```" + msg + "```"
}
function infoMessage(msg) {
    return "⏺️ *x-Troid*:  ```" + msg + "```"
}


module.exports = {
    successfullMessage,
    errorMessage,
    infoMessage
}
